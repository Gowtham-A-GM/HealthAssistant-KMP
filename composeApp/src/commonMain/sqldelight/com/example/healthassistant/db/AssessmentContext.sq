CREATE TABLE AssessmentContext (
    question_id TEXT NOT NULL PRIMARY KEY,
    question_text TEXT NOT NULL,
    response_type TEXT NOT NULL,
    response_options_json TEXT,
    answer_json TEXT NOT NULL
);

selectAll:
SELECT * FROM AssessmentContext;

insertContext:
INSERT OR REPLACE INTO AssessmentContext(
    question_id,
    question_text,
    response_type,
    response_options_json,
    answer_json
)
VALUES (?, ?, ?, ?, ?);

clearAll:
DELETE FROM AssessmentContext;
