CREATE TABLE PredefinedAnswer (
    question_id TEXT NOT NULL PRIMARY KEY,
    question_text TEXT NOT NULL,
    options_json TEXT NOT NULL,
    selected_answer TEXT NOT NULL
);

selectAll:
SELECT * FROM PredefinedAnswer;

insertAnswer:
INSERT OR REPLACE INTO PredefinedAnswer(
    question_id,
    question_text,
    options_json,
    selected_answer
)
VALUES (?, ?, ?, ?);

clearAll:
DELETE FROM PredefinedAnswer;
